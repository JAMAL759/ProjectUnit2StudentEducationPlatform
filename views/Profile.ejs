        </ul>

    </nav>
    
    
    </div>

    <div  id = "mainGrid" >

    <% allStudentAvailable.forEach( (obj) => { %>


        <div id="centerGrid"> 

            <div id="header1">
                
         
                
                
                </div>
        
        
        
            <div id="cover">
        
           
               
                <div id="centerGridCard">
                    <h3> mainCard</h3>
                </div>
            
                <div id="buttonSection">
                    <% obj._id %>
                    <a href="/student/Profile/<%=obj._id%>">  <%= obj.name%> </a>
                </div>
            
            </div>
        
        
            </div>

            


        
    
        <%         })%> 








